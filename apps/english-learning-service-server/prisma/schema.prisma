datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Word {
  cluster   String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  isLearned Boolean?
  meaning   String?
  mnemonic  String?
  text      String?
  updatedAt DateTime @updatedAt
}
